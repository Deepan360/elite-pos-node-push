          document.getElementById('productCount').textContent = data.row_count; // Access 'row_count' instead of 'productCount'
        } else {
          console.error('Error fetching product count:', data.message);
        }
      })
      .catch(error => console.error('Error fetching product count:', error));

    // Fetch purchase count
    fetch('/auth/index/purchaseCount')
      .then(response => response.json())
      .then(data => {
        if (data.success) {
          document.getElementById('purchaseCount').textContent = data.row_count; // Access 'row_count' instead of 'companyName'
        } else {
          console.error('Error fetching purchase count:', data.message);
        }
      })
      .catch(error => console.error('Error fetching purchase count:', error));

  </script>
